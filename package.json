{
  "name": "react-router",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev --host",
    "start": "cross-env NODE_ENV=production node server.js",
    "typecheck": "react-router typegen && tsc",
    "podman:build": "podman build -t my-fe:latest .",
    "podman:save": "podman save --format oci-archive -o my-fe.tar my-fe:latest",
    "clean:unused": "podman image prune -f --filter dangling=true",
    "deploy:podman": "npm run build && npm run podman:build && npm run clean:unused && npm run podman:save",
    "docker:build": "docker build -t my-fe:latest .",
    "docker:save": "docker save -o my-fe.tar my-fe:latest",
    "docker:prune": "docker image prune -f --filter dangling=true",
    "deploy:docker": "npm run build && npm run docker:build && npm run docker:prune && npm run docker:save",
    "bundle:fe": "node build-frontend-bundle.mjs",
    "git:solo": "git add . && git diff --cached --quiet || git commit -m \"Update project\" && git pull origin main --no-rebase && git push origin main",
    "git:team": "git add . && git diff --cached --quiet || git commit -m \"Update project\" && git pull origin main --no-rebase && git push origin $(git branch --show-current)",
    "git:new": "git checkout -b"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-router/express": "^7.9.6",
    "@react-router/node": "^7.5.3",
    "@tanstack/react-query": "^5.77.2",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "express": "^5.1.0",
    "framer-motion": "^12.14.0",
    "isbot": "^5.1.27",
    "lucide-react": "^0.511.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.4",
    "react-router": "^7.5.3",
    "react-router-dom": "^7.6.1",
    "react-window": "^2.1.0",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.0",
    "zod": "^3.25.30",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "@react-router/dev": "^7.5.3",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/react-window": "^1.8.8",
    "dotenv": "^16.5.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.7",
    "tw-animate-css": "^1.3.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4"
  }
}
